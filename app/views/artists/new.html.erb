<h1>Add an Artist</h1>
<%= form_with(model: [@song, @artist], local: true, class: "ui form") do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, required: true %>
  <br />
  <%= f.label :genre %>
  <%= f.text_field :benre, required: true %>
  <br />
  <br />
  <%= f.submit nil, class: "ui button blue" %>
<% end %>

